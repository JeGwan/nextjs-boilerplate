{
  "presets": [
    [
      "next/babel",
      {
        "targets": {
          "edge": "17",
          "firefox": "60",
          "chrome": "67",
          "safari": "11.1",
          "ie": "11"
        },
        "useBuiltIns": "usage"
      }
    ]
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "@components/*": "./components/*",
          "@lib/*": "./lib/*",
          "@styles/*": "./styles/*"
        }
      }
    ],
    ["styled-components", { "ssr": true }]
  ]
}
